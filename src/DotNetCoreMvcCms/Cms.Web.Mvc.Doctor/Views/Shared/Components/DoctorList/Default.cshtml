@model List<Cms.Data.Models.Entities.DoctorEntity>

<div class="row shuffle-wrapper portfolio-gallery">
    @foreach (var item in Model)
    {

        <div class="col-lg-3 col-sm-6 col-md-6 mb-4 shuffle-item" data-groups="[&quot;cat1&quot;,&quot;cat2&quot;]">
            <div class="position-relative doctor-inner-box">
                <div class="doctor-profile">
                    <div class="doctor-img">
                        <img src="@item.ResimYolu" alt="doctor-image" class="img-fluid w-100">
                    </div>
                </div>
                <div class="content mt-3">
                    <h4 class="mb-0"><a href="doctor-single.html">@item.Name</a></h4>
                    <p>@item?.Category?.Name</p>
                </div>
            </div>
        </div>

    }
</div>